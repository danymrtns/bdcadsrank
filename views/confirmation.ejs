    function printDiv(divName) {
      let printContents = document.getElementById(divName).innerHTML;
      let originalContents = document.body.innerHTML;

      document.body.innerHTML = printContents;

      window.print();

      document.body.innerHTML = originalContents;
    }

    function enregistrerFormulaire() {
      var formData = {
        raisonSocial: document.getElementById('raisonSocial').value,
        nom: document.getElementById('nom').value,
        telephone: document.getElementById('telephone').value,
        email: document.getElementById('email').value,
        url: document.getElementById('url').value,
        client: document.getElementById('client').value,
        montant: document.getElementById('montant').value
      };

      var jsonData = JSON.stringify(formData, null, 2);

      var fs = require('fs');
      fs.writeFile('data/formulaire.json', jsonData, function (err) {
        if (err) {
          console.error(err);
          return;
        }
        console.log('Le formulaire a été enregistré avec succès.');
      });
    }

    function saveFormDataToJson() {
      const formulaire = document.getElementById('formulaire');
      const formData = new FormData(formulaire);
      const formObject = {};

      for (let [key, value] of formData.entries()) {
        formObject[key] = value;
      }

      const jsonData = JSON.stringify(formObject);

      // Envoie les données JSON au serveur
      fetch('/save-form-data', {
        method: 'POST',
        headers: {
          'Content-Type': '/data/formulaire.json'
        },
        body: jsonData
      })
        .then(response => {
          if (response.ok) {
            // Succès - rediriger vers la page de confirmation
            window.location.href = '/confirmation';
          } else {
            // Erreur - afficher un message d'erreur ou effectuer une autre action
            console.error('Erreur lors de l\'enregistrement des données');
          }
        })
        .catch(error => {
          console.error('Erreur lors de la requête fetch', error);
        });
    }


  </script>
