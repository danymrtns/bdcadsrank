  <script>
    function getCurrentDate() {
      var date = new Date();
      var day = date.getDate();
      var month = date.getMonth() + 1;
      var year = date.getFullYear();

      // Ajouter un zéro devant les jours/mois < 10
      if (day < 10) {
        day = '0' + day;
      }
      if (month < 10) {
        month = '0' + month;
      }

      var formattedDate = day + '/' + month + '/' + year;
      return formattedDate;
    }

    // JavaScript Bouton Modifier
    document.getElementById('modifierBtn').addEventListener('click', function () {
      var formulaire = document.getElementById('formulaire');
      formulaire.classList.toggle('hidden');
    });


    function downloadPDF() {
      let element = document.getElementById('printableArea');

      // Configuration de la conversion en PDF
      let opt = {
        margin: 0,
        filename: 'bon_de_commande <%= raisonSocial %>-<%= currentDate %>-<%= orderNumber %>.pdf',
        html2canvas: { scale: 2 },
        jsPDF: { unit: 'mm', format: 'a4', orientation: 'portrait' },
        logo: '/logo_adsrank_bdc.png',
      };

      // Attente de 1 seconde avant la conversion en PDF
      setTimeout(function () {
        // Conversion en PDF
        html2pdf().set(opt).from(element).save();
      }, 1000);
    }

